                        <!--                            <td colspan="1" style="border:1px solid black;width:100px"><center><b>Signature</b></center></td>-->
                        <!--                        </tr>-->
                        <!--                    </table>-->
                        <hr style="width:100%;" color="orange"></hr>
                        <div class="row">
                            <div class="col-12" style="font-size:11px">
                                <t t-esc="company.email"/>-<t t-esc="company.ar_phone"/>-<t t-esc="company.ar_country"/>
                                ,<t t-esc="company.ar_street2"/>,<t t-esc="company.ar_street"/>-<t
                                    t-esc="company.company_registry"/>-سجل الشركة
