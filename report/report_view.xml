<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="hasa_invoice_format">
        <t t-call="web.html_container">
            <t t-if="not o" t-set="o" t-value="doc"/>
            <t t-if="not company">
                <!--  Multicompany  -->
                <t t-if="company_id">
                    <t t-set="company" t-value="company_id"/>
                </t>
                <t t-elif="o and 'company_id' in o">
                    <t t-set="company" t-value="o.company_id.sudo()"/>
                </t>
                <t t-else="else">
                    <t t-set="company" t-value="res_company"/>
                </t>
            </t>
            <div class="header" t-att-style="report_header_style" style="font-size:12px;">
                <div class="row">
                    <div class="col-6">
                        <img t-if="company.logo" t-att-src="image_data_uri(company.logo)"
                             style="height:200px;width:200px;align:left;" alt="Logo"/>
                    </div>


                    <div class="col-6" style="text-align:right;">
                        <img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('QR', o.testing(), 100, 100)"
                             style="width:100px;height:100px"/>

                        <!--            <h6><t t-esc="company.partner_id.name"/> </h6>-->
                        <!--            <h6><t t-esc="company.street"/> </h6>-->
                        <!--            <h6><t t-esc="company.street2"/> </h6>-->
                        <!--            <h6><t t-esc="company.city"/> </h6>-->
                        <!--            <h6><t t-esc="company.state_id.name"/> </h6>-->
                        <!--            <h6><t t-esc="company.zip"/> </h6>-->


                    </div>
                    <!---->
                </div>
                <div class="row">
                    <div class="col-12">
