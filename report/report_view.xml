                                    <center>
                                        <b>PO No.&amp;nbsp;&amp;nbsp;رقم الطلب</b>
                                    </center>
                                </td>
                            </tr>
                            <tr>
                                <!--                    <td colspan="1" style="border:1px solid black"><center><t t-esc="o.date_invoice"/></center></td>-->
                                <td colspan="1" style="border:1px solid black">
                                    <center>
                                        <t t-esc="o.invoice_date"/>
                                    </center>
                                </td>
                                <!--                    <td colspan="1" style="border:1px solid black"><center><span t-field="o.invoice_date" t-options='{"format": "dd-MM-yyyy"}'/></center></td>-->
                                <td colspan="1" style="border:1px solid black">
                                    <center>
                                        <t t-esc="o.payment_reference"/>
                                    </center>
                                </td>
                                <td colspan="1" style="border:1px solid black">
                                    <center>
                                        <t t-esc="o.po_date"/>
                                    </center>
                                </td>
                                <!--                    <td colspan="1" style="border:1px solid black;border-right:0px"><t t-esc="o.partner_id.city"/> </td>-->
                                <!--                    <td colspan="1" style="border:1px solid black;border-left:0px">-->
                                <!--&lt;!&ndash;                        <t t-esc="o.picking_policy"/>&ndash;&gt;-->
                                <!--                    </td>-->
                                <td colspan="1" style="border:1px solid black">
                                    <center>
                                        <t t-esc="o.po_number"/>
                                    </center>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>

            </div>
            <hr style="width:100%;" color="orange"></hr>

            <div class="col-9 text-right" style="margin-top:22px;" t-field="company.report_header" name="moto">
                <div t-field="company.partner_id"
                     t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": true}'/>
            </div>
            <div class="article" t-att-data-oe-model="o and o._name" t-att-data-oe-id="o and o.id"
                 t-att-data-oe-lang="o and o.env.context.get('lang')">
                <t t-raw="0"/>
            </div>
            <div class="footer o_background_footer">
                <div>
                    <div t-field="company.report_footer"/>
                    <div t-if="report_type == 'pdf'" class="text-muted">
